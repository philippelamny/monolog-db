{
    "name": "philippelamny/monolog-db",
    "description": "Laravel 6.x Monolog custom database channel",
    "keywords": ["laravel", "laravel6", "log", "monolog", "database"],
    "homepage": "https://github.com/philippelamny/monolog-db",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe Lam",
            "email": "philippe.lam.ny@gmail.com"
        },
		{
            "name": "Michael Tombrello",
            "email": "gordonzero@gmail.com"
        },
        {
            "name": "Mark Hilton",
            "email": "nerd305@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-0": {
            "Logger": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Logger\\Laravel\\Provider\\MonologMysqlHandlerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}